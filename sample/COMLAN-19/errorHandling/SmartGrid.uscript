class Substation {
	Fuse[] fuse;
	
	Cable[] getCables() {
		Cable[] cables = new Cable[fuse.length];
		for(int i=0; i<cables.length; i=i+1) {
			Fuse f = fuse[i];
			cables[i] = f.cable;
		}
	}
}

class Cabinet extends Substation{}

class DeadEndCabinet extends Substation{}

